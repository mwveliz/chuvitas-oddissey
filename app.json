{
  "expo": {
    "name": "Uchuva's Odyssey",
    "slug": "chuvitas-odyssey",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "assets/images/icon.png",
    "scheme": "chuvitasodyssey",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#962FBC"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chuvitasodyssey.game"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/images/adaptive-icon.png",
        "backgroundColor": "#7B2CBF"
      },
      "package": "com.chuvitasodyssey.game",
      "versionCode": 4
    },
    "web": {
      "bundler": "metro",
      "favicon": "assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "557be0e8-d3a2-4564-8b90-89829467f724"
      }
    }
  }
}